 |-- days_credit_update: integer (nullable = true)
 |-- amt_annuity: double (nullable = true)
 |-- sk_id_bureau: integer (nullable = true)
 |-- month_balance: byte (nullable = true)
 |-- status: string (nullable = true)
 |-- id: integer (nullable = true)


create table export_balance_bureau(
sk_id_curr int,
sk_id_bureau int,
credit_active varchar(255),
credit_currency varchar(255),
days_credit int,
credit_day_overdue int,
days_credit_enddate FLOAT(20,10),
days_enddate_fact FLOAT(20,10),
amt_credit_max_overdue FLOAT(20,10),
cnt_credit_prolong int,
amt_credit_sum FLOAT(20,10),
amt_credit_sum_debt FLOAT(20,10),
amt_credit_sum_limit FLOAT(20,10),
amt_credit_sum_overdue FLOAT(20,10),
credit_type varchar(255),
days_credit_update int,
amt_annuity FLOAT(20,10),
month_balance int,
new_status varchar(255));

